{
    "activationEvents": [
        "*"
    ],
    "author": {
        "name": "Simon Chan",
        "url": "https://github.com/yume-chan"
    },
    "bugs": {
        "url": "https://github.com/yume-chan/vscode-svn/issues"
    },
    "categories": [
        "SCM Providers"
    ],
    "contributes": {
        "commands": [
            {
                "category": "Svn",
                "command": "svn.update",
                "title": "%command.update%"
            },
            {
                "category": "Svn",
                "command": "svn.commit",
                "icon": {
                    "dark": "resources/icons/dark/check.svg",
                    "light": "resources/icons/light/check.svg"
                },
                "title": "%command.commit%"
            },
            {
                "category": "Svn",
                "command": "svn.stage",
                "icon": {
                    "dark": "resources/icons/dark/stage.svg",
                    "light": "resources/icons/light/stage.svg"
                },
                "title": "%command.stage%"
            },
            {
                "category": "Svn",
                "command": "svn.stageAll",
                "icon": {
                    "dark": "resources/icons/dark/stage.svg",
                    "light": "resources/icons/light/stage.svg"
                },
                "title": "%command.stageAll%"
            },
            {
                "category": "Svn",
                "command": "svn.unstage",
                "icon": {
                    "dark": "resources/icons/dark/unstage.svg",
                    "light": "resources/icons/light/unstage.svg"
                },
                "title": "%command.unstage%"
            },
            {
                "category": "Svn",
                "command": "svn.unstageAll",
                "icon": {
                    "dark": "resources/icons/dark/unstage.svg",
                    "light": "resources/icons/light/unstage.svg"
                },
                "title": "%command.unstageAll%"
            },
            {
                "category": "Svn",
                "command": "svn.update",
                "title": "%command.update%"
            },
            {
                "category": "Svn",
                "command": "svn.refresh",
                "icon": {
                    "dark": "resources/icons/dark/refresh.svg",
                    "light": "resources/icons/light/refresh.svg"
                },
                "title": "%command.refresh%"
            },
            {
                "category": "Svn",
                "command": "svn.openFile",
                "icon": {
                    "dark": "resources/icons/dark/open-file.svg",
                    "light": "resources/icons/light/open-file.svg"
                },
                "title": "%command.openFile%"
            }
        ],
        "menus": {
            "scm/resourceGroup/context": [
                {
                    "command": "svn.stageAll",
                    "group": "inline",
                    "when": "scmProvider == svn && scmResourceGroup != staged"
                },
                {
                    "command": "svn.stageAll",
                    "group": "1_modification",
                    "when": "scmProvider == svn && scmResourceGroup != staged"
                },
                {
                    "command": "svn.unstageAll",
                    "group": "inline",
                    "when": "scmProvider == svn && scmResourceGroup == staged"
                },
                {
                    "command": "svn.unstageAll",
                    "group": "1_modification",
                    "when": "scmProvider == svn && scmResourceGroup == staged"
                }
            ],
            "scm/resourceState/context": [
                {
                    "command": "svn.stage",
                    "group": "1_modification",
                    "when": "scmProvider == svn && scmResourceGroup != staged"
                },
                {
                    "command": "svn.stage",
                    "group": "inline",
                    "when": "scmProvider == svn && scmResourceGroup != staged"
                },
                {
                    "command": "svn.unstage",
                    "group": "1_modification",
                    "when": "scmProvider == svn && scmResourceGroup == staged"
                },
                {
                    "command": "svn.unstage",
                    "group": "inline",
                    "when": "scmProvider == svn && scmResourceGroup == staged"
                },
                {
                    "command": "git.openFile",
                    "group": "navigation",
                    "when": "scmProvider == svn"
                }
            ],
            "scm/title": [
                {
                    "command": "svn.refresh",
                    "group": "navigation",
                    "when": "scmProvider == svn"
                },
                {
                    "command": "svn.commit",
                    "group": "navigation",
                    "when": "scmProvider == svn"
                }
            ]
        }
    },
    "dependencies": {},
    "description": "",
    "devDependencies": {
        "@types/mocha": "^2.2.43",
        "@types/node": "^8.0.28",
        "mocha": "^4.0.0",
        "tslint": "^5.7.0",
        "tslint-language-service": "^0.9.6",
        "typescript": "^2.5.2",
        "vscode": "^1.1.5"
    },
    "displayName": "Svn",
    "engines": {
        "vscode": "^1.16.0"
    },
    "license": "MIT",
    "main": "./out/extension",
    "name": "vscode-svn",
    "publisher": "cnsimonchan",
    "repository": {
        "type": "git",
        "url": "https://github.com/yume-chan/vscode-svn"
    },
    "scripts": {
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "vscode:build": "node-gyp -C svn build --target=1.7.7 --arch=x64 --dist-url=https://atom.io/download/electron --debug",
        "vscode:prepublish": "node-gyp -C svn rebuild --target=1.7.7 --arch=x64 --dist-url=https://atom.io/download/electron --debug && node svn/scripts/postinstall.js debug && tsc -p ./"
    },
    "version": "0.0.1"
}
